	savePath: '%tempDir%/sessions'


# application settings
application:


	# application mapping
	mapping:
		*: Module\*\*Presenter


# enabling system container for Tracy bar
di:
	debugger: yes


# application extension
extensions:
	- Drago\Parameters\ParametersExtension
